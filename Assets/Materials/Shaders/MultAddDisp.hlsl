void MultAddDisp_float(
    float3 Displacement1, float3 Displacement2, float3 Displacement3, float3 Displacement4, float3 Displacement5, float3 Displacement6, float3 Displacement7, float3 Displacement8,
    out float3 Displacement)
{
    Displacement = Displacement1 + Displacement2 + Displacement3 + Displacement4 + Displacement5 + Displacement6 + Displacement7 + Displacement8;
}